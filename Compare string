//Compare two strings
#include <stdio.h>
#include <string.h>

int main() {
char a[50],b[50];
int n;
printf("Enter your first string:");
scanf("%s",a);
printf("Enter your second string:");
scanf("%s",b);
n=strcmp(a,b);
if(n==0)
{
    printf("Strings are equal");
}
else
{
    printf("Strings are not equal");
}
return 0;
}
