#include <stdio.h>
void multi(int a)
{
    int limit;
    printf("Enter the limit");
    scanf("%d",&limit);
    printf("Table\n");
    for(int i=1;i<=limit;i++)
    {
        printf("%d*%d=%d\n",i,a,i*a);
    }
}
    int main()
    {
        int num;
        printf("Enter the number");
        scanf("%d",&num);
        multi(num);
    return 0;
}
