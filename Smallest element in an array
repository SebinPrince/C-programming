#include <stdio.h>
int main() {
int i,j,a[50],n,min=0;
printf("Enter a number:");
scanf("%d",&n);
printf("Enter the array elements:\n");
for(i=0;i<n;i++)
{
    scanf("%d",&a[i]);
}
printf("Array elements:\n");

for(i=0;i<n;i++)
{
    printf("%d\t\n",a[i]);
}
min=a[0];
for(i=0;i<n;i++)
{
    if (min>a[i])
    {
        min=a[i];
    }
}
printf("Smallest is %d",min);
    return 0;
}
