#include<stdio.h>
int main()
{
int i,j,n,a[50],count=0;
printf("Enter the limit:");
scanf("%d",&n);
printf("Enter the array elemets:\n");
for(i=0;i<n;i++)
{
    scanf("%d",&a[i]);
}
printf("Array elements are:\n");
for(i=0;i<n;i++)
{
    printf("%d\t",a[i]);
}
printf("\nArray elements in reverse order:\n");
for(i=n-1;i>=0;i--)
{
    printf("%d\t",a[i]);
}
return 0;
}
