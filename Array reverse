#include<stdio.h>
int main()
{
int i,j,n,a[50],count=0;
printf("Enter the limit:");
scanf("%d",&n);
printf("Array elemets is:");
for(i=0;i<n;i++)
{
    scanf("%d",&a[i]);
}
printf("elements:");
for(i=0;i<n;i++)
{
    printf("%d\t",a[i]);
}
printf("\nArray eleents in reverse order:");
for(i=n-1;i>=0;i--)
{
    printf("%d\t",a[i]);
}
return 0;
}
